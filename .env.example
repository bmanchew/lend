# Didit API Configuration
CLIENT_ID=your_didit_client_id
CLIENT_SECRET=your_didit_client_secret
SHARED_SECRET_KEY=your_didit_webhook_secret

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Server Configuration
PORT=5000
NODE_ENV=development

# Security
SESSION_SECRET=your_session_secret
CORS_ORIGIN=http://localhost:5000

# Optional Features
ENABLE_RATE_LIMITING=true
MAX_REQUESTS_PER_WINDOW=100
RATE_LIMIT_WINDOW_MS=900000

# Twilio Configuration (for OTP)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# KYC Service Configuration
KYC_API_KEY=your_kyc_api_key
KYC_WEBHOOK_SECRET=your_kyc_webhook_secret

# Test Admin Configuration (Development Only)
ADMIN_TEST_EMAIL=admin@example.com
ADMIN_TEST_PASSWORD=change_me_in_production